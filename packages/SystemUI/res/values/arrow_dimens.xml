<resources>

    <dimen name="volume_dialog_panel_transparent_padding_left">8dp</dimen>

    <!-- Network traffic -->
    <dimen name="network_traffic_width">25sp</dimen>

    <!-- Width of the battery icon in the status bar when set to circle style. -->
    <dimen name="status_bar_battery_icon_circle_width">14.5dp</dimen>

    <!-- Height of the big battery circle icon in the status bar. -->
    <dimen name="status_bar_battery_bigcircle_icon_height">20.5dp</dimen>

    <!-- Width of the big battery circle icon in the status bar. -->
    <dimen name="status_bar_battery_bigcircle_icon_width">20.5dp</dimen>

    <!-- Bluetooth dialog -->
    <dimen name="bluetooth_dialog_active_device_bottom_margin">4dp</dimen>

    <!-- Auth ripple animation duration
         AOSP default is 1533 -->
    <item name="auth_ripple_animation_duration" format="float" type="dimen">766.5</item>
    
    <!-- Maximum vertical offset of statusbar for burn-in protection -->
    <dimen name="status_bar_offset_max_y">4dp</dimen>

    <!-- Screenrecord -->
    <dimen name="screenrecord_dot_size">48dp</dimen>

</resources>
